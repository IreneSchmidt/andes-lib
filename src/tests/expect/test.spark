Configuration {
        software_name: "Projeto de Teste"
        about: "Descrição do meu projeto de Teste"
        language: arquitetura-solicitada-pelo-usuário
}

// Descrição do Módulo 1
module Módulo 1 {
        // Descrição do Pacote 1
        module Pacote1 {
                entity Escola {
                        Nome: string unique blank

                }

                entity Matricula {
                        codigo: string

                }

                entity Aluno {
                        Nome: string max: 10 min: 10
                        SemestreIngresso uses Semestre

                        matricula OneToMany Matricula
                }

                enum Semestre {
                        primeiro
                        segundo
                }
        }
}